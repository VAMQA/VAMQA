<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,Log4net"/>
  </configSections>
  <appSettings>
    <add key="SDCUserName" value="DEV\SVC-Node-Service-Web" />
    <add key="SDCPassword" value="jEveXu2US7APusus" />    
    <add key="Environment" value="ED1"></add>
    <add key="Environment" value="DV1"></add>
    <add key="Environment" value="SI1"></add>
    <add key="Environment" value="PT1"></add>
    <add key="Environment" value="EI1"></add>
    <add key="Environment" value="EU1"></add>
    <add key="Environment" value="IN1"></add>
    <add key="Environment" value="UT1"></add>
    <add key="Environment" value="FT1"></add>
    <!--resistricted {core/payement}-->
    <add key="LOB" value="AUTO"></add>   
   </appSettings> 
  <log4net>
    <logger name="Logger">
      <appender-ref ref="XmlAppender"/>
      <level value="ALL"/>
    </logger>
    <appender name="XmlAppender" type="log4net.Appender.FileAppender">
      <file type="log4net.Util.PatternString" value="%property{LogName}.log"/>
      <immediateFlush value="true"/>
      <appendToFile value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%M %C] - %message%newline"/>
      </layout>
    </appender>
  </log4net>
</configuration>
  